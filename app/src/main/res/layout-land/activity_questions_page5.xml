<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".questions_page5">

    <TextView
        android:id="@+id/questions10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="48dp"
        android:text="@string/QDesc10"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/questions11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="64dp"
        android:text="@string/QDesc11"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/questions10"
        app:layout_constraintTop_toTopOf="@+id/questions10" />

    <RatingBar
        android:id="@+id/answer11"
        style="@android:style/Widget.Holo.RatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="45dp"
        android:contentDescription="@string/QDesc11"
        android:rating="3"
        android:stepSize="0.5"
        app:layout_constraintStart_toStartOf="@+id/questions11"
        app:layout_constraintTop_toBottomOf="@+id/questions11" />

    <Button
        android:id="@+id/backButtonPage5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:onClick="back"
        android:text="@string/ButtonBack5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/NextButtonPage5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/NextButtonPage5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:onClick="next"
        android:text="@string/BuutonNext5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/backButtonPage5" />

    <TextView
        android:id="@+id/questions12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/QDesc12"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/questions10"
        app:layout_constraintTop_toBottomOf="@+id/answer10titleDontKnow" />

    <Spinner
        android:id="@+id/answer12"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:entries="@array/array_animal_answer"
        app:layout_constraintStart_toStartOf="@+id/questions12"
        app:layout_constraintTop_toBottomOf="@+id/questions12"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <SeekBar
        android:id="@+id/answer10"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="274dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:max="10"
        android:progress="1"
        app:layout_constraintStart_toStartOf="@+id/questions10"
        app:layout_constraintTop_toBottomOf="@+id/questions10" />

    <TextView
        android:id="@+id/answer10titleNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ADesc10No"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/answer10"
        app:layout_constraintTop_toBottomOf="@+id/answer10" />

    <TextView
        android:id="@+id/answer10titleYes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ADesc10Yes"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/answer10"
        app:layout_constraintTop_toBottomOf="@+id/answer10" />

    <TextView
        android:id="@+id/answer10titleDontKnow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ADesc10DontKnow"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/answer10"
        app:layout_constraintStart_toStartOf="@+id/answer10"
        app:layout_constraintTop_toBottomOf="@+id/answer10" />

</androidx.constraintlayout.widget.ConstraintLayout>